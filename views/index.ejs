<!DOCTYPE html>
<html>
    
    <body>
       
        <h1>Pokedex Home</h1>
        <nav>
            <a href="/new">Add a new Pokemon</a>
        </nav>
        <ul>
            <% for(let i = 0; i < data.length; i++){ %>

                <li>
                    <a href="/<%= i %>">
                    <img src=
                     "<%= data[i].img %>"
                        alt=" Broken " >
                        </a>
                    
                        <form action="/<%= i %>?_method=DELETE" method="POST">
                        <input type="submit" value="DELETE">
                    </form>
                    <a href="/<%= i %>/edit">EDIT</a>
                </li>

            <% } %>
        </ul>
       
    </body>
</html>